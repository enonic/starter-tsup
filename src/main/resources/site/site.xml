<site>
	<form>
		<input name="sampleProperty" type="TextLine">
			<label>Site-wide property</label>
		</input>
	</form>
	<processors>
		<response-processor name="sampleProcessor" order="10"/>
		<response-processor name="browserSync" order="99"/>
	</processors>
	<mappings>
		<mapping controller="/site/static.js" order="50">
			<pattern>/posten-static/.+</pattern>
		</mapping>
	</mappings>
</site>
